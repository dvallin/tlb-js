{
  "name": "tlb-js",
  "version": "1.0.0",
  "description": "Time Loop Bandits in JS",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack -p",
    "test": "tsc --noEmit && jest --config=jest.conf.js --coverage",
    "test:watch": "jest --config=jest.conf.js --watchAll --coverage",
    "format": "prettier --config .prettierrc.yaml --write '{src,configs,test}/**/*.{ts,tsx,js,jsx,html}'",
    "format:check": "prettier --config .prettierrc.yaml --list-different '{src,configs,test}/**/*.{ts,tsx,js,jsx,html}'"
  },
  "dependencies": {
    "random-seed": "^0.3.0",
    "rot-js": "^2.0.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/node": "10.11.0",
    "@types/random-seed": "^0.3.3",
    "@types/rot-js": "^0.6.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "jest-canvas-mock": "^2.0.0-alpha.3",
    "prettier": "^1.16.4",
    "ts-jest": "^23.10.5",
    "typescript": "^3.3.3",
    "webpack": "^4.29.3",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.2",
    "webpack-dev-server": "^3.1.14"
  }
}
