{
  "name": "tlb-js",
  "version": "1.0.0",
  "description": "Time Loop Bandits in JS",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack -p",
    "test": "tsc --noEmit && jest --config=jest.conf.js --coverage",
    "test:watch": "jest --config=jest.conf.js --watchAll --coverage",
    "format": "prettier --config .prettierrc.yaml --write '{src,configs,test}/**/*.{ts,tsx,js,jsx,html}'",
    "format:check": "prettier --config .prettierrc.yaml --list-different '{src,configs,test}/**/*.{ts,tsx,js,jsx,html}'"
  },
  "dependencies": {
    "logic-solver": "^2.0.1",
    "permissive-fov": "^2.0.0",
    "random-seed": "^0.3.0",
    "rot-js": "^2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^25.1.4",
    "@types/node": "13.9.0",
    "@types/random-seed": "^0.3.3",
    "@types/rot-js": "^2.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "jest-canvas-mock": "^2.2.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
